<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Book Collection</title>
    <link rel="stylesheet" href="styles/reset.css"  type="text/css">
    <link rel="stylesheet" href="styles/base.css"  type="text/css">
    <link rel="stylesheet" href="styles/layout.css"  type="text/css">
    <link rel="stylesheet" href="styles/icons.css"  type="text/css">
    <link rel="stylesheet" href="styles/modules.css"  type="text/css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@1,500&family=Roboto:wght@500&display=swap" rel="stylesheet">
  </head>
  <body>
    <main>
      <h4 id="title">Best Books!</h4>
      <div id="container">
        <% bookObject2.forEach( book => { %>
            <img src="<%= book.image %>" alt="<% book.title %>"/>
            <h5> <%= book.title %></h5>
            <h6> <%= book.author %></h6>
            <h7> <%= book.isbn %></h7>
            <button>Return to home</button>
        <% }) %>
      </div>
    </main>
  </body>
</html>